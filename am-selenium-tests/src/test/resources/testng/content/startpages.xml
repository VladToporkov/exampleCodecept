            <package name="com.actionmedia.tests.content.startpages.fss.*"/>
        </packages>
    </test>

    <test name="ГФ" preserve-order="true">
        <packages>
            <package name="com.actionmedia.tests.content.startpages.gf.*"/>
        </packages>
    </test>

    <test name="ОТ" preserve-order="true">
        <packages>
            <package name="com.actionmedia.tests.content.startpages.ehs.*"/>
