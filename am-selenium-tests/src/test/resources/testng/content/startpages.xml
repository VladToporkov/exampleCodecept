            <package name="com.actionmedia.tests.content.startpages.actualis_edu.*"/>
        </packages>
    </test>

    <test name="ACT: Кадровое дело" preserve-order="true">
        <packages>
            <package name="com.actionmedia.tests.content.startpages.actualis_k.*"/>
        </packages>
    </test>

    <test name="ACT: Госзакупки" preserve-order="true">
        <packages>
            <package name="com.actionmedia.tests.content.startpages.actualis_gz.*"/>
        </packages>
    </test>

</suite>
