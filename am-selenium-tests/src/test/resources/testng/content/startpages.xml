<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="startpages" verbose="10" parallel="tests" thread-count="20">

    <listeners>
        <listener class-name="com.actionmedia.autotest.listeners.NewStartPagesMailSenderListener"/>
        <listener class-name="com.actionmedia.autotest.listeners.CuantoListener"/>
    </listeners>

    <test name="БСС" preserve-order="true">
        <packages>
            <package name="com.actionmedia.tests.content.startpages.bss.*"/>
        </packages>
    </test>

    <test name="КСС" preserve-order="true">
        <packages>
