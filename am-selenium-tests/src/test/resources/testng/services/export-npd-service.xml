<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke" verbose="10" parallel="tests" thread-count="20">

    <listeners>
        <listener class-name="com.actionmedia.autotest.listeners.SkipTestListener"/>
    </listeners>

    <test name="export.npd.service.tests" preserve-order="true">
        <groups>
            <run>
                <include name="service"/>
                <include name="npd"/>
            </run>
        </groups>
        <classes>
            <class name="com.actionmedia.tests.services.exportnpdservice.ExportNPDServiceTests"/>
            <class name="com.actionmedia.tests.services.exportnpdservice.ExportNPDServiceAttachesTests"/>
        </classes>
    </test>

</suite>
