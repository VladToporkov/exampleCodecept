<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression" verbose="1" parallel="tests" thread-count="20">

    <listeners>
        <listener class-name="com.actionmedia.autotest.listeners.CuantoListener"/>
    </listeners>

    <test name="bss ku" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.BSS_KU_Warm"/>
        </classes>
    </test>

    <test name="bss bu" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.BSS_BU_Warm"/>
        </classes>
    </test>

    <test name="bss usn" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.BSS_USN_Warm"/>
        </classes>
    </test>

    <test name="bss vip" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.BSS_VIP_Warm"/>
        </classes>
    </test>

    <test name="kss ku" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.KSS_KU_Warm"/>
