    </test>

    <test name="kss vip" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.KSS_VIP_Warm"/>
        </classes>
    </test>

    <test name="uss ku" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.USS_KU_Warm"/>
        </classes>
    </test>

    <test name="uss bu" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.USS_BU_Warm"/>
        </classes>
    </test>

    <test name="fss" preserve-order="true">
        <classes>
            <class name="com.actionmedia.tests.experimental.warmsystems.WarmFss"/>
        </classes>
    </test>

</suite>
