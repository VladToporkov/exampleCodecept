<?xml version="1.0" encoding="UTF-8"?>
<root>
    <event-parameters event-type="AccessActivation">
        <event-parameter> <!-- алиас типа события -->
            <name>alias</name>
            <value>AccessActivation</value>
        </event-parameter>
        <event-parameter> <!-- идентификатор пользователя -->
            <name>user_token</name>
            <value>STUB_USER_TOKEN</value>
        </event-parameter>
        <event-parameter> <!-- дата события -->
            <name>date</name>
            <value>STUB_DATE</value>
        </event-parameter>
        <event-parameter> <!-- идентификатор издания -->
            <name>pub_id</name>
            <value>STUB_PUB_ID</value>
        </event-parameter>
        <event-parameter> <!-- код активации доступа -->
            <name>crm_card</name>
            <value>STUB_CRM_CARD</value>
        </event-parameter>
    </event-parameters>
</root>
